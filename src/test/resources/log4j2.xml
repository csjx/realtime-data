<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyyMMdd-HH:mm:ss}: [%p]: %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="edu.hawaii.soest" level="info">
            <AppenderRef ref="STDOUT" />
        </Logger>
        <Root level="error">
            <AppenderRef ref="STDOUT" />
        </Root>
    </Loggers>
</Configuration>